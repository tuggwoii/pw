@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Master.cshtml";

    IPublishedContent node = Umbraco.TypedContent(1050);
}
<div class="subpage">
    <div class="row">
        <h1><i class="fa fa-volume-control-phone" aria-hidden="true"></i> @Umbraco.Field("title")</h1>
    </div>
    <div class="container suppage-container">
        <div class="row">
            <div class="col-lg-12">
                @Umbraco.Field("text")
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6">
                <h2><i class="fa fa-volume-control-phone"></i> ช่องทางการติดต่อ</h2>
                <div class="contact-info">
                    <p class="phone"><i class="fa fa-volume-control-phone" aria-hidden="true"></i> โทรศัพท์: @node.GetPropertyValue("phone")</p>
                    <p class="email"><i class="fa fa-envelope-o" aria-hidden="true"></i> email: @node.GetPropertyValue("email")</p>
                </div>
            </div>
            <div class="col-lg-6">
                <h2><i class="fa fa-map-marker" aria-hidden="true"></i> ที่อยู่</h2>
                <div class="address">
                    @node.GetPropertyValue("address")
                </div>
            </div>
            <div class="col-lg-12">
                <h2><i class="fa fa-map"></i>  แผนที่ ที่ตั้งร้าน</h2>
                <div class="embed-responsive embed-responsive-16by9">
                    <iframe class="embed-responsive-item" src="https://www.google.com/maps/d/embed?mid=1jBroCpzVMlZQV853BcB0k4LkgmA" width="640" height="480"></iframe>
                </div>
            </div>
        </div>
    </div>
</div>
